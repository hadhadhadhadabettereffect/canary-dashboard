{
  "extends": ["tslint:recommended", "tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "public/*.js"
    ]
  },
  "rules": {
    "only-arrow-functions": false,
    "trailing-comma": false,
    "no-consecutive-blank-lines": false,
    "no-bitwise": false,
    "jsx-no-multiline-js": false,
    "no-console": false,
    "jsx-boolean-value": false,
    "curly": false
  }
}
